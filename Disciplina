package br.thiagomouro.academico;

/**
 *
 * @author thiago
 */
public class Disciplina{
    private String nome;
    private String cargaHoraria;
    private String serieAssociada;
}

public Disciplina(String nome, String cargaHoraria, string serieAssociada) {
        this.nome = nome;
        this.cargaHoraria = cargaHoraria;
        this.serieAssociada = serieAssociada    
    }

 public String getNome() {
        return nome;
    }


 public void setNome(String nome) {
        this.nome = nome;
    }

